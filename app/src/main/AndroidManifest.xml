<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flobberworm.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".suspension.SimpleListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".suspension.recycle.SimpleRecycleActivity"></activity>
        <activity
            android:name=".parallax.ParallaxActivity"
            android:theme="@style/AppThemeParallax" />
        <activity android:name=".digital.DigitalActivity" />
        <activity android:name=".parabola.ParabolaActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>